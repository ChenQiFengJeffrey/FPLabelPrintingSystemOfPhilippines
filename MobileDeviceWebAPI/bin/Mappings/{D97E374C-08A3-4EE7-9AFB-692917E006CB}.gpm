    "columns": [ 
        {
            xtype: 'combocolumn',                                           
            dataIndex: 'material',
            text: 'Description ',
            displayField:'name'
        },
        {            
            header: "Quantity",
            dataIndex: "amount",
            hidden: false,
            format: "0",
            width: 80,
            editor: {
                'xtype': 'numberfield',
                'allowBlank': false,
                'decimalPrecision': 0,
                'minValue':1 
            },
            'renderer':function(val, metaData, record){
                if(val == record.get("maxamount"))
                    return val;
                    else
                    return val + "/" + record.get("maxamount");
            } 
        },      
        {
            xtype: 'combocolumn',                                           
            dataIndex: 'package',
            text: 'UOM',
            displayField:'name'        
        },
