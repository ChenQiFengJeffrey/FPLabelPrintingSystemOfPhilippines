            width:140
        },
        {                                          
            dataIndex: 'iscomplete',
            text: 'Whether Completed',
            displayField:'name',
            'renderer':function(val, metaData, record){
                if(val === false)
                    return 'No';
                    else
                    return 'Yes';
            } 
        },

        {
            xtype: 'combocolumn',                                           
